includes:
    - ./vendor/larastan/larastan/extension.neon

parameters:
    tmpDir: cache

    paths:
        - app

    scanFiles:

    # The level 9 is the highest level
    level: 8

    parallel:
        maximumNumberOfProcesses: 4

#    ignoreErrors:
#        - '#Unsafe usage of new static#'

    checkFunctionNameCase: true
    checkUninitializedProperties: true
